@model IEnumerable<DataLayer.contactemail>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.contact.firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.emailaddress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.contact.firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.emailaddress)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.contactemailid }) |
            @Html.ActionLink("Details", "Details", new { id=item.contactemailid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.contactemailid })
        </td>
    </tr>
}

</table>
